{
  "name": "react-bootstrap-scss-boilerplate",
  "version": "1.0.0",
  "description": "react-bootstrap-scss-boilerplate",
  "main": "src/index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development && npm-run-all clean && webpack serve",
    "build:dev": "cross-env NODE_ENV=development && npm-run-all clean && webpack",
    "build:prod": "cross-env NODE_ENV=production && npm-run-all clean && webpack",
    "clean": "rm -rf build && mkdir build",
    "lint-fix": "eslint ./src/**/*.ts --fix & eslint ./src/**/*.tsx --fix & stylelint ./src/**/*.scss --fix",
    "lint": "npm-run-all lint:*",
    "lint:tsc": "npx tsc -p tsconfig.json --skipLibCheck",
    "lint:src": "npx eslint --ext=.ts,.tsx ./",
    "lint:style": "npx stylelint **/*.scss",
    "test": "npm-run-all lint",
    "postinstall": "npx husky install"
  },
  "dependencies": {
    "bootstrap": "5.3.2",
    "react": "18.2.0",
    "react-bootstrap": "2.9.2",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/react": "18.2.47",
    "@types/react-dom": "18.2.18",
    "@typescript-eslint/eslint-plugin": "6.18.1",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "12.0.1",
    "cross-env": "7.0.3",
    "css-loader": "6.9.0",
    "eslint": "8.56.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-eslint-plugin": "5.2.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-webpack-plugin": "4.0.1",
    "file-loader": "6.2.0",
    "fork-ts-checker-webpack-plugin": "9.0.2",
    "html-webpack-plugin": "5.6.0",
    "husky": "8.0.3",
    "mini-css-extract-plugin": "2.7.7",
    "node-sass": "9.0.0",
    "npm-run-all": "4.1.5",
    "sass-loader": "13.3.3",
    "style-loader": "3.3.4",
    "stylelint": "16.1.0",
    "stylelint-config-standard-scss": "13.0.0",
    "ts-loader": "9.5.1",
    "typescript": "5.3.3",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1"
  }
}
